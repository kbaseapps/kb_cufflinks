{
  "ver": "1.0.0",
  "authors": [],
  "contact": "help@kbase.us",
  "visible": true,
  "categories": [
    "active",
    "expression"
  ],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output": "no-display"
  },
  "parameters": [
    {
      "id": "expressionset_ref",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseRNASeq.RNASeqExpressionSet", "KBaseSets.ExpressionSet"
        ]
      }
    },{
      "id": "input_type",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [ "genes" ],
      "field_type": "dropdown",
      "dropdown_options": {
          "options": [{
              "display": "Genes",
              "value": "genes"
          }, {
              "display": "Transcripts",
              "value": "transcripts"
          }]
      }
  }, {
      "id": "output_obj_name",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseSets.DifferentialExpressionMatrixSet"
        ],
        "is_output_name": true
      }
    },
    {
    "id" : "library_norm_method",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "classic-fpkm" ],
    "field_type" : "dropdown",
    "dropdown_options":{
      "options": [
        {
          "value": "geometric",
          "display": "geometric"
        },
        {
          "value": "classic-fpkm",
          "display": "no scaling to FPKM"
        },
        {
          "value": "quartile",
          "display": "quartile"
        }
     ]
    }
    },
    {
      "id": "multi_read_correct",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        "0"
      ],
      "field_type": "checkbox",
      "checkbox_options": {
        "checked_value": 1,
        "unchecked_value": 0
      }
    },
    {
      "id": "time_series",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        "0"
      ],
      "field_type": "checkbox",
      "checkbox_options": {
        "checked_value": 1,
        "unchecked_value": 0
      }
    },
    {
      "id": "min_alignment_count",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        "10"
      ],
      "field_type": "text",
      "text_options": {
        "validate_as": "int",
        "min_int": 1
      }
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "kb_cufflinks",
      "method": "run_Cuffdiff",
      "input_mapping": [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter": "expressionset_ref",
          "target_type_transform": "resolved-ref",
          "target_property": "expressionset_ref"
        },
        {
          "input_parameter" : "input_type",
          "target_property" : "input_type"
        },
        {
          "input_parameter": "min_alignment_count",
          "target_property": "min_alignment_count"
        },
        {
          "input_parameter": "multi_read_correct",
          "target_property": "multi_read_correct"
        },
        {
          "input_parameter": "time_series",
          "target_property": "time_series"
        },
        {
          "input_parameter": "library_norm_method",
          "target_property": "library_norm_method"
        },
        {
          "input_parameter": "output_obj_name",
          "target_property": "output_obj_name"
        }
      ],
      "output_mapping": [
        {
          "input_parameter": "output_obj_name",
          "target_property": "output_obj_name"
        },
        {
          "service_method_output_path": [0, "result_directory"],
          "target_property": "result_directory"
        },
        {
          "service_method_output_path": [0, "diffExprMatrixSet_ref"],
          "target_property": "diffExprMatrixSet_ref"
        },
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        },
        {
          "constant_value": "16",
          "target_property": "report_window_line_height"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}